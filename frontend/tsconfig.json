{
    "extends": "./tsconfig.paths.json",
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "jsx": "react",
      "moduleResolution": "node",
      "strict": true,
      "noEmit": true,
      "allowJs": true,
      "skipLibCheck": true,
      "isolatedModules": true,
      "esModuleInterop": true
    },
    "exclude": ["./node_modules/**/*"],
    "include": ["src"]
}